{
  "name": "aideator",
  "version": "0.1.0",
  "description": "AI-powered CLI tool for brainstorming and refining project ideas.",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "node dist/index.js",
    "build": "node_modules/.bin/tsc",
    "dev": "node_modules/.bin/tsc -w",
    "test": "jest",
    "lint": "node_modules/.bin/eslint ./src/**/*.ts",
    "lint:fix": "node_modules/.bin/eslint ./src/**/*.ts --fix",
    "format": "node_modules/.bin/prettier --write ./src/**/*.ts",
    "format:check": "node_modules/.bin/prettier --check ./src/**/*.ts",
    "test:cov": "jest --coverage"
  },
  "keywords": [
    "ai",
    "cli",
    "ideation",
    "brainstorming",
    "project planning"
  ],
  "author": "AIdeator Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.29",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "jest": "^29.7.0",
    "pino-pretty": "^13.0.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.3.4",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "pino": "^9.7.0"
  }
}
